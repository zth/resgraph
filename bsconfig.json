{
  "name": "rescript-relay-router",
  "version": "0.1.0",
  "uncurried": true,
  "sources": [
    {
      "dir": "cli",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "suffix": ".mjs",
  "bs-dependencies": ["@rescript/core", "rescript-nodejs"],
  "bsc-flags": ["-open NodeJs", "-open RescriptCore"],
  "js-post-build": {
    "cmd": "./scripts/post-build.sh"
  }
}
