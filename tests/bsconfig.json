{
  "name": "test",
  "uncurried": true,
  "reanalyze": {
    "analysis": ["dce"]
  },

  "package-specs": {
    "in-source": true,
    "module": "es6",
    "suffix": ".mjs"
  },
  "sources": [
    {
      "dir": "../src/res",
      "subdirs": true
    },
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "tests",
      "subdirs": true
    }
  ],
  "bsc-flags": ["-w -33-44", "-open RescriptCore"],
  "bs-dependencies": [
    "@rescript/react",
    "@rescript/core",
    "@glennsl/rescript-fetch",
    "rescript-nodejs"
  ],
  "jsx": { "version": 4 }
}
